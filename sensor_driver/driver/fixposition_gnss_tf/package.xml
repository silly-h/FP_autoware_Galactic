<?xml version="1.0"?>
<package format="3">
    <name>fixposition_gnss_tf</name>
    <version>3.0.0</version>
    <description>Fixposition GNSS coordinates transformation</description>

    <maintainer email="info@fixposition.com">Fixposition AG</maintainer>

